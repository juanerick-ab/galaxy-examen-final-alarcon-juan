pipeline {
    agent any

    stages {
        stage('Inicio') {
            steps {
                echo '🚀 Iniciando pipeline de prueba...'
            }
        }

        stage('Verificación') {
            steps {
                echo '✅ Jenkins está funcionando correctamente.'
            }
        }

        stage('Final') {
            steps {
                echo '🎉 Pipeline de prueba finalizado con éxito.'
            }
        }
    }
}
